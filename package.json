{
  "name": "watchman-processor",
  "version": "0.0.10",
  "description": "Processor for the fb-watchman service",
  "author": "Markis Taylor <m@rkis.cc>",
  "main": "index.js",
  "bin": "./.bin/watchman-processor",
  "preferGlobal": true,
  "dependencies": {
    "chalk": "^1.1.3",
    "fb-watchman": "^1.9.0",
    "inversify": "^2.0.0-beta.6",
    "inversify-dts": "^1.0.1",
    "node-emoji": "^1.3.0",
    "promise": "^7.1.1",
    "reflect-metadata": "^0.1.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.10.2",
    "istanbul": "^0.4.3",
    "mocha": "^2.5.1",
    "mocha-lcov-reporter": "^1.2.0",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.4",
    "ts-helpers": "^1.1.1",
    "tslint": "^3.10.2",
    "typescript": "^1.8.10"
  },
  "scripts": {
    "compile": "tsc",
    "coveralls": "tsc && npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "coverage": "tsc && istanbul cover _mocha built/test/**/*-test.js",
    "lint": "tslint -c tslint.json src/*.ts test/*.ts",
    "mocha": "tsc && mocha test/_setup.js built/test/**/*-test.js",
    "test": "npm run coverage"
  },
  "pre-commit": [
    "lint",
    "coverage"
  ],
  "repository": "https://github.com/markistaylor/watchman-processor.git",
  "license": "ISC"
}
